{
  "name": "@ddn/bin",
  "version": "0.0.1",
  "description": "DDN development tools for DDN Blockchain",
  "main": "index.js",
  "bin": {
    "ddn-bin": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepack": "npm run build",
    "build": "tsc -b",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif readme && git add README.md",
    "install-local": "./install.sh"
  },
  "keywords": [
    "ddn",
    "blockchain",
    "cli",
    "scripts",
    "development"
  ],
  "author": "Imfly",
  "license": "MIT",
  "dependencies": {
    "@oclif/core": "^2.0.0",
    "@oclif/plugin-help": "^5.0.0",
    "oclif": "^4.17.43"
  },
  "devDependencies": {
    "@types/history": "^4.7.11",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.86",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "ddn-bin",
    "dirname": "ddn-bin",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "pnpm": {
    "overrides": {
      "@oclif/core": "^2.0.0"
    }
  }
}
